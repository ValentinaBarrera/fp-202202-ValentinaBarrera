Algoritmo Aleatorios
	Definir num1, num2 Como Entero
	
	Escribir " Dame un numero"
	Leer num1
	Escribir " Dame otro numero"
	Leer num2
	
	aleatorio1 <- AZAR (num1)
	aleatorio2 <- ALEATORIO(num1,num2)
	
	Escribir " Los numeros generados fueron", aleatorio1, " y ", aleatorio2
FinAlgoritmo
